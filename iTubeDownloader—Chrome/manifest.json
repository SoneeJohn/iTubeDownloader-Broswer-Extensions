{
  "background" : {
    "scripts" : [
      "background.js"
    ]
  },
  "browser_action" : {
    "default_icon" : "itubedownloader_16x16.png",
    "default_title" : "Send link to iTubeDownloader"
  },
  "commands" : {
    "ITDSendCurrentLink" : {
      "description" : "Send link to iTubeDownloader",
      "suggested_key" : {
        "default" : "Alt+Shift+D"
      }
    }
  },
  "description" : "Sends the current link to iTubeDownloader.",
  "icons" : {
    "128" : "icon128.png",
    "16" : "icon16.png",
    "48" : "icon48.png"
  },
  "manifest_version" : 2,
  "name" : "iTubeDownloader",
  "permissions" : [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version" : "1.1"
}
